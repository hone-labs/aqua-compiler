{
  "name": "aqua-compiler",
  "version": "0.0.1",
  "description": "A TEAL compiler and REPL.",
  "main": "index.js",
  "scripts": {
    "build-parser": "pegjs -o src/parser.js  --allowed-start-rules start,expression src/grammar.pegjs",
    "start": "npm run build-parser && ts-node ./src/cli.ts exec test.aqua",
    "start:repl": "npm run build-parser && ts-node ./src/cli.ts",
    "s": "npm run start",
    "start:dev": "npm run build-parser && nodemon",
    "c": "npm run clean",
    "clean": "rm -rf build/* && rm -rf tsconfig.tsbuildinfo",
    "b": "npm run build",
    "build": "tsc --incremental",
    "bw": "npm run build:watch",
    "build:watch": "tsc --incremental --watch",
    "prepublishOnly": "npm test && npm run clean && npm run build",
    "t": "npm run test",
    "test": "npm run build-parser && jest",
    "tw": "npm run test:watch",
    "test:watch": "npm run build-parser && jest --watch --runInBand",
    "build-win": "npm run prepublishOnly  && nexe build/cli.js -t windows-x64-12.16.2 -n aqua",
    "build-mac": "npm run prepublishOnly && nexe build/cli.js -t mac-x64-12.16.2 -n aqua-mac",
    "build-linux": "npm run prepublishOnly && nexe build/cli.js -t linux-x64-12.16.2 -n aqua-linux",
    "build-all": "npm run build-win && npm run build-mac && npm run build-linux"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "private",
  "dependencies": {
    "@nodelib/fs.walk": "^1.2.8",
    "algosdk": "scale-it/js-algorand-sdk.git#0f9d09e8d0bddc37145013e538b3ddfd001d364f",
    "lodash.clonedeep": "^4.5.0",
    "minimist": "^1.2.5",
    "pegjs": "^0.10.0",
    "sha3": "^2.1.4",
    "tweetnacl-ts": "^1.0.3",
    "yaml": "^1.10.2",
    "zod": "^1.10.2"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/jest": "^27.0.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^14.17.18",
    "@types/superagent": "^4.1.13",
    "dedent": "^0.7.0",
    "jest": "^27.2.1",
    "nodemon": "^2.0.12",
    "source-map-support": "0.5.20",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "nexe": "^4.0.0-beta.18"
  }
}
