{
  "name": "aqua-compiler",
  "version": "1.0.0",
  "description": "A TEAL compiler and REPL.",
  "main": "index.js",
  "scripts": {
    "build-parser": "pegjs -o src/parser.js src/grammar.pegjs",
    "start": "npm run build-parser && node ./build/cli.js",
    "start:dev": "npm run build-parser && nodemon",
    "c": "npm run clean",
    "clean": "rm -rf build/*",
    "b": "npm run build",
    "build": "tsc --incremental",
    "bw": "npm run build:watch",
    "build:watch": "tsc  --incremental --watch",
    "prepublishOnly": "npm test && npm run clean && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "t": "npm run test",
    "test": "jest",
    "tw": "npm run test:watch",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "private",
  "dependencies": {
    "minimist": "^1.2.5",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.14",
    "dedent": "^0.7.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.12",
    "source-map-support": "0.5.19",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  }
}
